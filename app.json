{
  "expo": {
    "jsEngine": "hermes",
    "name": "frontend-task",
    "slug": "frontend-task",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./server/img/favicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./server/img/favicon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./server/img/favicon.png"
    }
  }
}
